{
    "manifest_version": 3,
    "name": "Label Exporter Extension",
    "version": "1.0",
    "description": "Export labels of elements into CSV",
    "permissions": [
        "scripting",
        "downloads"
      ],
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
        "*://*/*"
      ]
  }